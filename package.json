{
  "name": "watchlist",
  "description": "What to watch",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "clean:build": "rimraf build && rimraf public",
    "clean:node_modules": "rimraf node_modules",
    "clean": "npm run clean:build && npm run clean:node_modules",
    "reinstall": "npm run clean && npm install",
    "start": "./node_modules/.bin/nodemon --exec NODE_ENV=development ./node_modules/.bin/babel-node ./server/index.js",
    "lint": "eslint .",
    "build:client": "NODE_ENV=production webpack --config ./webpack.config.js --progress --profile --colors",
    "build:server": "./node_modules/.bin/babel -d ./build ./server -s",
    "build": "npm run lint && npm run clean:build && npm run build:server && npm run build:client",
    "production-build": "npm run build && npm run clean:node_modules && npm install --production",
    "production": "NODE_ENV=production node ./build/index.js",
    "test:client": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register test/app/**",
    "test:client:watch": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register --watch test/app/**",
    "test:server": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register test/server/**",
    "test:server:watch": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register --watch test/server/**",
    "test": "npm run lint && npm run test:client && npm run test:server"
  },
  "dependencies": {
    "async": "1.5.2",
    "debug-caller": "2.1.0",
    "express": "4.13.3",
    "glob": "6.0.4",
    "history": "1.17.0",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.0.0",
    "mongoose": "4.3.6",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-redux": "4.0.6",
    "react-router": "1.0.3",
    "redux": "3.0.5",
    "redux-actions": "0.9.1",
    "redux-logger": "2.4.0",
    "redux-simple-router": "1.0.2",
    "redux-thunk": "1.0.3",
    "request": "2.67.0",
    "slug": "0.9.1"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-strict-mode": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "css-loader": "0.23.1",
    "eslint": "2.6.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.2.3",
    "fetch-mock": "4.3.0",
    "html-webpack-plugin": "1.7.0",
    "mocha": "2.4.5",
    "node-sass": "3.4.2",
    "nodemon": "1.8.1",
    "react-transform-hmr": "1.0.1",
    "redux-mock-store": "1.0.2",
    "rewire": "2.5.1",
    "rimraf": "2.5.0",
    "sass-loader": "3.1.2",
    "should": "8.2.1",
    "style-loader": "0.13.0",
    "webpack": "1.12.14",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0"
  }
}
